<Window x:Class="DatabaseApplication.GUI.Views.MainView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:DatabaseApplication.GUI.ViewModel"
        xmlns:view="clr-namespace:DatabaseApplication.GUI.Views"
        Title="MainViews" Height="300" Width="300">
    <Grid>
        <ListBox ItemsSource="{Binding  CountryList}">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <StackPanel Orientation="Horizontal">
                        <TextBox Text="{Binding id}"></TextBox>
                        <TextBox Text="{Binding name}"></TextBox>
                    </StackPanel>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
    </Grid>
</Window>
